          <TableColumn fx:id="idVaccin" prefWidth="75.0" text="idVaccin" />
          <TableColumn fx:id="nomVaccin" prefWidth="75.0" text="nomVaccin" />
            <TableColumn fx:id="periodeVaccin" prefWidth="75.0" text="periode" />
            <TableColumn fx:id="quantiteVaccin" prefWidth="75.0" text="quantiteVaccin" />
