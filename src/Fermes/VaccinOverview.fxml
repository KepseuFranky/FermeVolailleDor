      <Button layoutX="557.0" layoutY="574.0" mnemonicParsing="false" onAction="#handleNewVaccin" text="New" />
      <Label layoutX="59.0" layoutY="32.0" prefHeight="25.0" prefWidth="105.0" text="Vaccin" />
