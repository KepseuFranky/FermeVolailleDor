            <TableColumn fx:id="prixVaccin" prefWidth="75.0" text="prixVaccin" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button layoutX="706.0" layoutY="574.0" mnemonicParsing="false" onAction="#handleDeleteVaccin" text="Delete" />
